{
  "name": "prueba-npm-nextjs",
  "version": "1.0.4",
  "description": "Un paquete de componentes React optimizado para Next.js 15 con App Router",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "license": "MIT",
  "author": "Tu Nombre",
  "keywords": [
    "nextjs",
    "nextjs-15",
    "react",
    "componente",
    "app-router",
    "typescript"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=18 <21",
    "react-dom": ">=18 <21",
    "next": ">=14"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
